# EditorConfig  http://EditorConfig.org

# Top-most EditorConfig file
root = true

# All files
[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

# JavaScript
[*.js]
indent_size=2

# Yaml
[*.{yaml, yml}]
indent_size=2
